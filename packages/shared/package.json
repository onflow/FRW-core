{
  "name": "@onflow/flow-wallet-shared",
  "version": "0.0.1",
  "description": "Shared types and utilities for Flow Reference Wallet",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "build:watch": "tsup --watch",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "prepublishOnly": "pnpm build"
  },
  "exports": {
    ".": {
      "types": "./src/index.ts",
      "default": "./src/index.ts"
    },
    "./types": {
      "types": "./src/types/index.ts",
      "default": "./src/types/index.ts"
    },
    "./constants": {
      "types": "./src/constants/index.ts",
      "default": "./src/constants/index.ts"
    },
    "./utils": {
      "types": "./src/utils/index.ts",
      "default": "./src/utils/index.ts"
    }
  },
  "dependencies": {
    "@onflow/fcl": "^1.13.1",
    "@onflow/typedefs": "^1.4.0",
    "@onflow/types": "^1.4.0",
    "bignumber.js": "^9.1.2"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "tsup": "^8.3.5",
    "typescript": "^5.7.2"
  },
  "peerDependencies": {
    "ethers": "^6.13.4"
  },
  "engines": {
    "node": ">=22.11.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/onflow/FRW-Extension2.git",
    "directory": "packages/shared"
  },
  "license": "MIT"
}
